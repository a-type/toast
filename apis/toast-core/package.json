{
  "name": "toast-core",
  "version": "0.1.0",
  "description": "Toast API",
  "main": "src/server.js",
  "scripts": {
    "dev:run": "node --inspect=9229 -r ts-node/register src/server.ts",
    "dev": "cross-env NODE_PATH=src nodemon",
    "dev:mock": "cross-env NODE_PATH=src nodemon --config nodemon-mock.json",
    "test": "cross-env NODE_PATH=src jest",
    "db:index": "node ./db/indexes.js",
    "db-init": "run-s db:*",
    "build:clean": "rimraf build",
    "build:transpile": "tsc",
    "build": "run-s build:*",
    "build-client:pack": "cd ../../apps/toast-web && npm run build",
    "build-client:copy": "cp -r ../../apps/toast-web/dist ./client",
    "build-client": "run-s build-client:*",
    "containerize:client": "npm run build-client",
    "containerize:run": "docker build -t toast-core .",
    "containerize:clean": "rimraf client",
    "containerize": "run-s containerize:*",
    "start": "cross-env NODE_PATH=build node build/server.js",
    "mock": "npm run dev:mock"
  },
  "repository": {
    "type": "git",
    "url": "git+https://gitlab.com/a-type/toast-backend.git"
  },
  "keywords": [
    "Toast"
  ],
  "author": "Grant Forrest <gaforres@gmail.com>",
  "license": "ISC",
  "bugs": {
    "url": "https://gitlab.com/a-type/toast-backend/issues"
  },
  "homepage": "https://gitlab.com/a-type/toast-backend#README",
  "dependencies": {
    "@google-cloud/firestore": "^0.17.0",
    "@google-cloud/storage": "^1.7.0",
    "apollo-server-express": "^2.0.6",
    "auth0": "^2.12.0",
    "aws-sdk": "^2.229.1",
    "bcryptjs": "^2.4.3",
    "blob-to-stream": "^1.0.3",
    "body-parser": "^1.18.2",
    "casual": "^1.5.19",
    "chalk": "^2.4.1",
    "change-case": "^3.0.2",
    "compromise": "^11.12.0",
    "compromise-plugin": "0.0.8",
    "config": "^1.30.0",
    "convert-units": "^2.3.4",
    "cors": "^2.8.4",
    "cross-env": "^5.1.4",
    "date-fns": "^1.29.0",
    "express": "^4.16.3",
    "express-jwt": "^5.3.1",
    "graphql": "^0.13.2",
    "graphql-parse-resolve-info": "^4.0.0-rc.12",
    "graphql-playground-middleware-express": "^1.6.1",
    "graphql-tools": "^2.24.0",
    "imaginary": "^0.1.16",
    "ingredients-parser": "^1.4.0",
    "js-quantities": "^1.7.2",
    "jsonwebtoken": "^8.2.1",
    "jwks-rsa": "^1.3.0",
    "mime-types": "^2.1.18",
    "minimist": "^1.2.0",
    "neo4j-driver": "^1.7.1",
    "neo4j-graphql-binding": "^1.0.3",
    "neo4j-graphql-js": "^0.1.14",
    "node-fetch": "^2.2.0",
    "parse-unit": "^1.0.1",
    "ramda": "^0.25.0",
    "shortid": "^2.2.12",
    "string-similarity": "^1.2.2",
    "text2num": "0.0.1",
    "uuid": "^3.2.1"
  },
  "devDependencies": {
    "@types/auth0": "^2.9.9",
    "@types/bcryptjs": "^2.4.2",
    "@types/express": "^4.16.0",
    "@types/jest": "^23.3.9",
    "@types/minimist": "^1.2.0",
    "@types/node-fetch": "^2.1.4",
    "@types/ramda": "^0.25.41",
    "@types/uuid": "^3.4.4",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-jest": "^23.4.2",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-stage-0": "^6.24.1",
    "jest": "^23.5.0",
    "nodemon": "^1.18.6",
    "npm-run-all": "^4.1.2",
    "prettier": "^1.14.3",
    "rimraf": "^2.6.2",
    "ts-jest": "^23.10.4",
    "ts-node": "^7.0.1",
    "typescript": "^3.1.6"
  }
}
