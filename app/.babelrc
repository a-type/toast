{
  "env": {
    "production": {
      "presets": [
        "@babel/preset-typescript",
        ["@babel/preset-env", {}],
        "@babel/preset-react",
        "@emotion/babel-preset-css-prop"
      ],
      "plugins": [
        "emotion",
        "@babel/plugin-proposal-object-rest-spread",
        "@babel/plugin-proposal-class-properties",
        "graphql-tag",
        "styled-components"
      ]
    },
    "development": {
      "presets": [
        "@babel/preset-typescript",
        ["@babel/preset-env", {}],
        "@babel/preset-react",
        "@emotion/babel-preset-css-prop"
      ],
      "plugins": [
        ["emotion", { "sourceMap": true }],
        "@babel/plugin-proposal-object-rest-spread",
        "@babel/plugin-proposal-class-properties",
        "graphql-tag",
        "styled-components"
      ]
    },
    "test": {
      "presets": [
        "@babel/preset-typescript",
        "env",
        "react",
        "@emotion/babel-preset-css-prop"
      ],
      "plugins": [
        "emotion",
        "@babel/plugin-proposal-object-rest-spread",
        "@babel/plugin-proposal-class-properties",
        "graphql-tag",
        "styled-components"
      ]
    }
  }
}
