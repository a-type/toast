    "toast-common": "^0.1.0",
    "typescript": "^3.5.2"
  },
  "devDependencies": {
    "@types/express": "^4.17.0",
    "@types/node": "^12.0.10",
    "@types/stripe": "^6.30.3",
    "cross-env": "^5.2.0",
    "npm-run-all": "^4.1.5",
    "ts-node-dev": "^1.0.0-pre.40",
    "typescript": "^3.5.2"
  }
