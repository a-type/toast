{
  "name": "toast-billing",
  "version": "1.0.0",
  "description": "Billing microservice for Toast",
  "main": "build/server.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "cross-env NODE_PATH=src ts-node-dev --respawn --no-deps --prefer-ts --inspect=127.0.0.1:9239 -r dotenv/config -- src/server.ts",
    "build:clean-dir": "rimraf build",
    "build:transpile": "tsc",
    "build": "run-s build:*",
