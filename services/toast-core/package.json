{
  "name": "toast-core",
  "version": "0.1.0",
  "description": "Toast Core API",
  "main": "src/server.js",
  "scripts": {
    "dev": "cross-env NODE_PATH=src ts-node-dev --respawn --no-deps --prefer-ts --inspect -r dotenv/config -- src/server.ts",
    "test": "cross-env NODE_PATH=src jest",
    "ci:test": "cross-env NODE_PATH=src jest",
    "db:index": "node ./db/indexes.js",
    "db-init": "run-s db:*",
    "make-admin": "ts-node scripts/makeAdmin.ts",
    "build:clean": "rimraf build",
    "build:transpile": "tsc",
    "build": "run-s build:*",
    "ci:build": "run-s build:*",
    "mock": "npm run dev:mock",
    "start": "cross-env NODE_PATH=build node -r dotenv/config build/server.js",
    "devenv:start": "gcloud compute instances start arangodb-1 --project=toast-cooking-dev",
    "devenv:stop": "gcloud compute instances stop arangodb-1 --project=toast-cooking-dev"
  },
  "repository": {
    "type": "git",
    "url": "git+https://gitlab.com/a-type/toast-core.git"
  },
  "keywords": [
    "Toast"
  ],
  "author": "Grant Forrest <gaforres@gmail.com>",
  "license": "ISC",
  "bugs": {
    "url": "https://gitlab.com/a-type/toast-core/issues"
  },
  "homepage": "https://gitlab.com/a-type/toast-core#README",
  "dependencies": {
    "@google-cloud/firestore": "^0.21.0",
    "@google-cloud/storage": "^2.3.4",
    "apollo-server-express": "^2.6.1",
    "arangojs": "^6.10.0",
    "auth0": "^2.12.0",
    "aws-sdk": "^2.229.1",
    "bcryptjs": "^2.4.3",
    "blob-to-stream": "^1.0.3",
    "body-parser": "^1.18.2",
    "casual": "^1.5.19",
    "chalk": "^2.4.1",
    "change-case": "^3.0.2",
    "compromise": "^11.12.0",
    "compromise-plugin": "0.0.8",
    "config": "^1.30.0",
    "convert-units": "^2.3.4",
    "cors": "^2.8.5",
    "cross-env": "^5.1.4",
    "dataloader": "^1.4.0",
    "date-fns": "^1.29.0",
    "dotenv": "^8.0.0",
    "express": "^4.16.3",
    "express-jwt": "^5.3.1",
    "firebase-admin": "^6.5.1",
    "graphql": "^14.4.2",
    "graphql-arangodb": "^0.1.11",
    "graphql-import": "^0.7.1",
    "graphql-middleware": "^3.0.2",
    "graphql-parse-resolve-info": "^4.0.0-rc.12",
    "graphql-playground-middleware-express": "^1.6.1",
    "graphql-tools": "^4.0.4",
    "imaginary": "^0.1.16",
    "ingredients-parser": "^1.4.0",
    "js-quantities": "^1.7.2",
    "jsonwebtoken": "^8.2.1",
    "jwks-rsa": "^1.3.0",
    "mime-types": "^2.1.18",
    "minimist": "^1.2.0",
    "node-fetch": "^2.2.0",
    "parse-unit": "^1.0.1",
    "ramda": "^0.25.0",
    "shortid": "^2.2.12",
    "string-similarity": "^1.2.2",
    "text2num": "0.0.1",
    "toast-common": "^0.1.0",
    "typescript": "^3.5.2",
    "uuid": "^3.2.1",
    "yup": "^0.26.10"
  },
  "devDependencies": {
    "@types/auth0": "^2.9.9",
    "@types/bcryptjs": "^2.4.2",
    "@types/express": "^4.16.0",
    "@types/jest": "^24.0.6",
    "@types/minimist": "^1.2.0",
    "@types/node-fetch": "^2.1.4",
    "@types/ramda": "^0.25.41",
    "@types/uuid": "^3.4.4",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-jest": "^24.1.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-stage-0": "^6.24.1",
    "dotenv": "^6.2.0",
    "jest": "^24.1.0",
    "nodemon": "^1.18.6",
    "npm-run-all": "^4.1.2",
    "prettier": "^1.14.3",
    "rimraf": "^2.6.2",
    "ts-jest": "^24.0.0",
    "ts-node": "^7.0.1",
    "ts-node-dev": "^1.0.0-pre.32",
    "typescript": "^3.1.6"
  }
}
